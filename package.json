  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:app": "eslint components services contexts hooks lib stores types utils --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:check": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:quality": "eslint --config eslint.config.quality.js .",
    "type-check": "tsc --noEmit -p tsconfig.app.json",
    "type-check:app": "tsc --noEmit -p tsconfig.app.json",
    "type-check:node": "tsc --noEmit -p tsconfig.node.json",
    "type-check:test": "tsc --noEmit -p tsconfig.test.json",
    "type-check:all": "npm run type-check && npm run type-check:node && npm run type-check:test",
    "type-check:strict": "tsc --noEmit --skipLibCheck false -p tsconfig.app.json",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "security:check": "npm audit --json",
    "security:validate": "npm run security:check",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "npm run build && npx vite-bundle-visualizer",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production') require('child_process').execSync('husky install')\"",
    "quality:check": "npm run lint:check && npm run type-check && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "deps:audit": "npm audit --audit-level moderate",
    "deps:fix": "npm audit fix",
    "clean": "rm -rf dist node_modules/.vite .tsbuildinfo",
    "fresh": "npm run clean && npm install",
    "fresh:modern": "npm run clean && npm install --omit=dev",
    "precommit": "lint-staged",
    "postinstall": "node -e \"if (process.env.NODE_ENV !== 'production' && require('fs').existsSync('.git')) require('child_process').execSync('husky install')\"",
    "cleanup:imports": "node scripts/cleanup-imports.js",
    "cleanup:all": "npm run cleanup:imports && npm run format",
    "test:e2e": "playwright test",
